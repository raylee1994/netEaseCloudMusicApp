{
    "presets": [
        ["env",{
            "modules": false,
            "targets": {
                "browsers": [
                    "last 2 versions",
                    "> 1%"
                ]
            }
        }],
        "stage-2",
        "react"
    ],
    "plugins": [
        "transform-runtime",
        "syntax-dynamic-import",
        "react-hot-loader/babel",
        ["import", {
            "libraryName": "antd", "libraryDirectory": "es", "style": true
        }],
        "transform-do-expressions"
    ]
}